{
  "private": true,
  "version": "1.20.0",
  "engines": {
    "node": ">= 10.6.0",
    "npm": ">= 6.1.0"
  },
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "lint": "eslint ./resources --ext .js,.vue; exit 0",
    "test": "ava ./resources/**/**.spec.js ./resources/assets/js/store/modules/**.spec.js",
    "test:bonusPro": "ava ./resources/assets/js/store/modules/*bonusPro**.spec.js",
    "test:e2e:local": "npx wdio wdio.local.conf.js",
    "test:e2e:local:ci": "npx wdio wdio.local.ci.conf.js",
    "test:e2e:bs": "npx wdio wdio.ci.conf.js",
    "test:prep": "mv ./resources/package.json ./resources/__package__.json",
    "test:clean": "mv ./resources/__package__.json ./resources/package.json",
    "pretest:e2e:local": "yarn test:prep",
    "posttest:e2e:local": "yarn test:clean",
    "pretest:e2e:local:ci": "yarn test:prep",
    "posttest:e2e:local:ci": "yarn test:clean",
    "pretest:e2e:bs": "yarn test:prep",
    "posttest:e2e:bs": "yarn test:clean",
    "app:install": "cd resources && yarn install",
    "app:build": "cd resources && yarn build"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/register": "^7.15.3",
    "@vue/test-utils": "^1.0.0-beta.29",
    "@wdio/browserstack-service": "^6.4.0",
    "@wdio/cli": "^6.1.25",
    "@wdio/local-runner": "^6.1.25",
    "@wdio/mocha-framework": "^6.1.19",
    "@wdio/spec-reporter": "^6.1.23",
    "@wdio/sync": "^6.1.14",
    "acorn": "^6.1.0",
    "ava": "^3.15.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.5",
    "browser-sync": "^2.26.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cross-env": "^5.1",
    "css-loader": "^2.1.0",
    "eslint": "^3.0.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-vue": "^2.1.0",
    "jsdom": "^16.6.0",
    "jsdom-global": "^3.0.2",
    "laravel-mix": "^4.0.7",
    "lodash": "^4.17.5",
    "popper.js": "^1.12",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0-beta.1",
    "require-extension-hooks-vue": "^3.0.0",
    "resolve-url-loader": "^2.3.1",
    "sass": "^1.15.2",
    "sass-loader": "^7.1.0",
    "selectn": "^1.1.2",
    "style-loader": "^0.23.1",
    "vue": "^2.6.6",
    "vue-loader": "^15.6.4",
    "vue-template-compiler": "^2.6.6",
    "vuex": "^3.1.0",
    "wdio-chromedriver-service": "^6.0.3",
    "wdio-intercept-service": "^4.1.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.1.0",
    "bootstrap-vue-dialog": "0.0.3",
    "chromedriver": "107.0.3",
    "jquery": "^3.2.1",
    "moment": "^2.24.0",
    "vue-date-pick": "^1.2.1",
    "vue-observe-visibility": "^1.0.0",
    "vue-spinner": "^1.0.4"
  },
  "ava": {
    "require": [
      "./tests/ava-setup.js"
    ]
  }
}
